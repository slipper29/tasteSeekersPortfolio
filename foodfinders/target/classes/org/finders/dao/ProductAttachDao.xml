<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="org.finders.dao.ProductAttachDao">
	<insert id="ProductAttachWrite">
		REPLACE INTO mas_attach(uuid,fileName,uploadPath,image,pno)
		values(#{uuid}, #{fileName}, #{uploadPath}, #{image}, #{pno})
	</insert>
</mapper>